alias gc="git commit -m"
alias gs="git status"
function ga {
  readonly path=${1:? "Must provide path"}
  git add "$path"
}
function gd {
  readonly path=${1:? "Must provide path"}
  git diff "$path"
}
function gr {
  readonly path=${1:? "Must provide path"}
  git rm "$path"
}
alias push="git push"
alias pull="gitpull"
